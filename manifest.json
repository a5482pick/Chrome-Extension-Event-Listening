{
  "manifest_version": 2,
  "name": "Content Script: Direct Communication",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
  "<all_urls>"]
}
